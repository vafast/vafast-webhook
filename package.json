{
  "name": "@vafast/webhook",
  "version": "0.0.15",
  "description": "Webhook dispatch middleware for Vafast framework - automatically trigger webhooks based on route configuration",
  "type": "module",
  "author": {
    "name": "vafast",
    "url": "https://github.com/vafast"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/vafast/vafast-webhook"
  },
  "main": "./dist/index.mjs",
  "module": "./dist/index.mjs",
  "types": "./dist/index.d.mts",
  "exports": {
    ".": {
      "types": "./dist/index.d.mts",
      "import": "./dist/index.mjs",
      "default": "./dist/index.mjs"
    }
  },
  "bugs": "https://github.com/vafast/vafast-webhook/issues",
  "homepage": "https://github.com/vafast/vafast-webhook",
  "keywords": [
    "vafast",
    "webhook",
    "middleware",
    "typescript",
    "event-driven"
  ],
  "scripts": {
    "build": "tsdown",
    "dev": "npx tsx watch example/index.ts",
    "test": "npx vitest run",
    "release": "npm run build && npm run test && npx bumpp && npm publish --access=public"
  },
  "license": "MIT",
  "files": [
    "dist",
    "README.md",
    "LICENSE"
  ],
  "devDependencies": {
    "@types/node": "^22.0.0",
    "tsdown": "^0.19.0-beta.4",
    "typescript": "^5.4.5",
    "vafast": "^0.5.1",
    "vitest": "^4.0.16"
  },
  "peerDependencies": {
    "vafast": "^0.5.1"
  }
}